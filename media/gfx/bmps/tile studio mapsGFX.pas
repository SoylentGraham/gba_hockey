  function GetBitmapFileName (s: string): string;

implementation

  function GetBitmapFileName (s: string): string;
  begin
    if s = 'Tiles1' then s := 'Gfx\Tiles1.bmp';
    if s = 'ice_rink_tiles' then s := 'Gfx\ice_rink_tiles.bmp';
    if s = 'rink_overhead_tiles' then s := 'Gfx\rink_overhead_tiles.bmp';
    if s = 'rink_screen_tiles' then s := 'Gfx\rink_screen_tiles.bmp';
    GetBitmapFileName := s;
  end;

end.